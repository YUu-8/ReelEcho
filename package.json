{
  "name": "devops-lab-student-project",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
  "start": "node src/index.js",
  "dev": "nodemon src/index.js",
  "test": "vitest run",
  "test:watch": "vitest",
  "test:unit": "vitest run test/unit", 
  "test:integration": "vitest run test/integration", 
  "test:coverage": "vitest run --coverage", 
  "lint": "eslint ."
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "mongodb": "^7.0.0",
    "mongoose": "^8.20.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@vitest/coverage-v8": "^2.1.9",
    "body-parser": "^2.2.1",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.5",
    "express": "^5.1.0",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "vitest": "^2.1.9"
  },
  "vitest": {
    "coverage": {
      "provider": "v8",
      "all": true,
      "include": [
        "src/**/*.js"
      ],
      "lines": 80,
      "functions": 80,
      "branches": 70,
      "statements": 80
    }
  }
}
